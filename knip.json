{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": ["app/**/*.{ts,tsx}", "lib/**/*.{ts,tsx}", "components/**/*.{ts,tsx}"],
	"project": ["**/*.{ts,tsx}"],
	"ignore": ["**/*.d.ts", ".next/**", "node_modules/**"],
	"ignoreDependencies": ["autoprefixer", "postcss", "tailwindcss", "tailwindcss-animate", "tw-animate-css"],
	"next": {
		"entry": ["app/**/*.{ts,tsx}", "middleware.{ts,tsx}"]
	}
}
